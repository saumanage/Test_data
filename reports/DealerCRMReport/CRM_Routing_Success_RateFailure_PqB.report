            <value></value>
        </criteriaItems>
        <criteriaItems>
            <column>Opportunity.Created_Date_DT__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>false</isUnlocked>
            <operator>equals</operator>
            <value>THIS_WEEK</value>
        </criteriaItems>
        <criteriaItems>
            <column>Opportunity.Routing_Status__c</column>
            <columnToColumn>false</columnToColumn>
            <isUnlocked>true</isUnlocked>
            <operator>equals</operator>
            <value>,Failure,Ready,Sent to Dealer,Submitted for Re-Routing</value>
