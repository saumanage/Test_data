<?xml version="1.0" encoding="UTF-8"?>
<Flow xmlns="http://soap.sforce.com/2006/04/metadata">
    <apiVersion>56.0</apiVersion>
    <decisions>
        <name>Oppty_workflow_to_populate_Dealer_Number</name>
        <label>Oppty workflow to populate Dealer Number</label>
        <locationX>641</locationX>
        <locationY>375</locationY>
        <defaultConnectorLabel>Default Outcome</defaultConnectorLabel>
        <rules>
            <name>Oppty_Preferred_Dealer_Account_Update_in_Portal</name>
            <conditionLogic>and</conditionLogic>
            <conditions>
                <leftValueReference>$Record.CreatedBy.UserType</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <stringValue>PowerPartner</stringValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.PreferredDealerAccount_TXT__c</leftValueReference>
                <operator>IsNull</operator>
                <rightValue>
                    <booleanValue>true</booleanValue>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Update_Preferred_Dealer_Account</targetReference>
            </connector>
            <label>Oppty Preferred Dealer Account Update in Portal</label>
        </rules>
        <rules>
            <name>update_Oppty_name</name>
            <conditionLogic>1 AND (2 OR 3 OR  4 OR 5 OR 6 OR 7 OR 8 OR 9 OR 10)</conditionLogic>
            <conditions>
                <leftValueReference>$Setup.BypassAutomations__c.BypassProcessBuilder__c</leftValueReference>
                <operator>EqualTo</operator>
                <rightValue>
                    <booleanValue>false</booleanValue>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.PreferredDealerAccount_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.PreferredDealerAccount_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.First_Name_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.First_Name_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Last_Name_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Last_Name_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record.LeadProvider_ID__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record__Prior.LeadProvider_ID__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Middle_Name_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Middle_Name_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Year_YR__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Year_YR__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Trim_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Trim_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Model_ID__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Model_ID__c</elementReference>
                </rightValue>
            </conditions>
            <conditions>
                <leftValueReference>$Record__Prior.Model_TXT__c</leftValueReference>
                <operator>NotEqualTo</operator>
                <rightValue>
                    <elementReference>$Record.Model_TXT__c</elementReference>
                </rightValue>
            </conditions>
            <connector>
                <targetReference>Populate_Dealer_Number_and_oppty_Name</targetReference>
            </connector>
            <label>update Oppty name</label>
        </rules>
    </decisions>
    <environments>Default</environments>
    <formulas>
        <name>OpportunityName</name>
        <dataType>String</dataType>
        <expression>LPAD({!$Record.First_Name_TXT__c}&amp;
IF({!$Record.Middle_Name_TXT__c}=Null,&quot; &quot;,&quot; &quot;&amp;{!$Record.Middle_Name_TXT__c}&amp;&quot; &quot;)&amp;
{!$Record.Last_Name_TXT__c}&amp;&quot; &quot;&amp;{!$Record.Year_YR__c }&amp;&quot; &quot;&amp;
IF({!$Record.Model_TXT__c }={!$Record.Trim_TXT__c },{!$Record.Model_TXT__c }&amp;&quot; &quot;&amp;{!$Record.Model_ID__c},{!$Record.Model_TXT__c }&amp;&quot; &quot;&amp;{!$Record.Model_ID__c}&amp;&quot; &quot;&amp;{!$Record.Trim_TXT__c }), 120)</expression>
    </formulas>
    <formulas>
        <name>PreferredDealerAccount</name>
        <dataType>String</dataType>
        <expression>{!$Record.CreatedBy.Account.Id}</expression>
    </formulas>
    <formulas>
        <name>PreferredDealerNumber</name>
        <dataType>String</dataType>
        <expression>{!$Record.PreferredDealerNumber_NUM__c}</expression>
    </formulas>
    <interviewLabel>Opportunity Record Update {!$Flow.CurrentDateTime}</interviewLabel>
    <label>Opportunity Record Update</label>
    <processMetadataValues>
