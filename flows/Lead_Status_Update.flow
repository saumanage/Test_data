    <description>If marketable flat changed to true and lead status = New or Manual Validation, then set Lead Status to Nurturing. 2. If Lead Grading changed to true and Lead status = New, Manual Validation, or Nurturing, then set Lead status to Ready to Convert 3. If Requires Manual Validation changed to true and lead status = new or nurturing, then set lead status to Manual Validation</description>
    <formulas>
        <processMetadataValues>
            <name>originalFormula</name>
            <value>
                <stringValue>AND ($Setup.BypassAutomations__c.BypassProcessBuilder__c = False ,

OR(
AND(
ISNEW(),
NOT(ISBLANK([Lead].LeadProvider_ID__c) ),
NOT(ISPICKVAL([Lead].LeadProvider_ID__r.Lead_Source__c, &apos;&apos;) ),
TEXT([Lead].LeadProvider_ID__r.Lead_Source__c) &lt;&gt; TEXT([Lead].LeadSource )
),

AND(
 ISCHANGED([Lead].LeadProvider_ID__c) ,
NOT(ISBLANK([Lead].LeadProvider_ID__c) ),
NOT(ISPICKVAL([Lead].LeadProvider_ID__r.Lead_Source__c, &apos;&apos;) ),
TEXT([Lead].LeadProvider_ID__r.Lead_Source__c) &lt;&gt; TEXT([Lead].LeadSource ))

)

)</stringValue>
            </value>
        </processMetadataValues>
        <name>formula_myRule_1</name>
        <dataType>Boolean</dataType>
        <expression>AND ({!$Setup.BypassAutomations__c.BypassProcessBuilder__c} = False ,

OR(
AND(
ISNEW(),
NOT(ISBLANK({!myVariable_current.LeadProvider_ID__c}) ),
NOT(ISPICKVAL({!myVariable_current.LeadProvider_ID__r.Lead_Source__c}, &apos;&apos;) ),
TEXT({!myVariable_current.LeadProvider_ID__r.Lead_Source__c}) &lt;&gt; TEXT({!myVariable_current.LeadSource} )
),

AND(
 ISCHANGED({!myVariable_current.LeadProvider_ID__c}) ,
NOT(ISBLANK({!myVariable_current.LeadProvider_ID__c}) ),
NOT(ISPICKVAL({!myVariable_current.LeadProvider_ID__r.Lead_Source__c}, &apos;&apos;) ),
TEXT({!myVariable_current.LeadProvider_ID__r.Lead_Source__c}) &lt;&gt; TEXT({!myVariable_current.LeadSource} ))

)

)</expression>
    </formulas>
