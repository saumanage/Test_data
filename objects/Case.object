    <validationRules>
        <fullName>Case_Type_Validation_rule</fullName>
        <active>true</active>
        <errorConditionFormula>(OR(TEXT( Type ) = &apos;CRM Switch&apos;,TEXT( Type ) = &apos;New Dealer CRM&apos;) &amp;&amp; (ISBLANK( New_CRM_System__c ) ) )</errorConditionFormula>
        <errorDisplayField>New_CRM_System__c</errorDisplayField>
        <errorMessage>Please provide the New CRM System for CRM switch request.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Lead_Receiving_Date_Should_Not_Be_Empty</fullName>
        <active>true</active>
        <description>Lead Receiving Date should not be empty.</description>
        <errorConditionFormula>(OR(TEXT( Type ) = &apos;CRM Switch&apos;,TEXT( Type ) = &apos;New Dealer CRM&apos;) &amp;&amp; (ISBLANK( CRM_switch_Start_Date__c ) ) ) &amp;&amp; $Setup.BypassAutomations__c.BypassValidationRule__c =False</errorConditionFormula>
        <errorDisplayField>CRM_switch_Start_Date__c</errorDisplayField>
        <errorMessage>Please select the Lead Receiving Start Date.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Restrict_Other_Division_for_CRM_Switch</fullName>
        <active>true</active>
        <errorConditionFormula>(CONTAINS(Text(Division__c),&apos;A&apos;) &amp;&amp; NOT(CONTAINS(Account.Active_Divisions__c, &apos;A&apos;)))||
(CONTAINS(Text(Division__c),&apos;B&apos;) &amp;&amp;  NOT(CONTAINS(Account.Active_Divisions__c, &apos;B&apos;))) ||
(CONTAINS(Text(Division__c),&apos;M&apos;) &amp;&amp; NOT(CONTAINS(Account.Active_Divisions__c, &apos;M&apos;))) ||
(CONTAINS(Text(Division__c),&apos;P&apos;) &amp;&amp; NOT(CONTAINS(Account.Active_Divisions__c, &apos;P&apos;)))</errorConditionFormula>
        <errorDisplayField>Division__c</errorDisplayField>
        <errorMessage>Please Select Relevant Division.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Selected_CRM_Status_Change_1</fullName>
        <active>true</active>
        <description>Step 5 can be selected only after step 4- Approved. The user should not be able to select Step 5 directly</description>
        <errorConditionFormula>OR(
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;CRM Vendor Confirmed&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Test Lead Routed Successfully&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Pending CRM Verification&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Test Lead Routed Successfully&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;New Request&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Test Lead Routed Successfully&quot;))
)</errorConditionFormula>
        <errorDisplayField>CRM_Switch_Status__c</errorDisplayField>
        <errorMessage>Selected CRM Status should be approved first.</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>Selected_CRM_Status_Change_2</fullName>
        <active>true</active>
        <description>CRM status can not be none.</description>
        <errorConditionFormula>OR(
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Test Lead Routed Successfully&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Approved&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Test Lead Routed Successfully&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;CRM Vendor Confirmed&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Test Lead Routed Successfully&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Pending CRM Verification&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Test Lead Routed Successfully&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;New Request&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Approved&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;CRM Vendor Confirmed&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Approved&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Pending CRM Verification&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Approved&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;New Request&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;CRM Vendor Confirmed&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;Pending CRM Verification&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;CRM Vendor Confirmed&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;New Request&quot;)),
AND(
ISPICKVAL( PRIORVALUE ( CRM_Switch_Status__c), &quot;Pending CRM Verification&quot;),
ISPICKVAL(CRM_Switch_Status__c, &quot;New Request&quot;))
)</errorConditionFormula>
        <errorDisplayField>CRM_Switch_Status__c</errorDisplayField>
        <errorMessage>Steps can not be reverted back.</errorMessage>
    </validationRules>
