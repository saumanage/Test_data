                    <default>false</default>
                    <label>Level 2</label>
                </value>
                <value>
                    <fullName>Level 3</fullName>
                    <default>false</default>
                    <label>Level 3</label>
                </value>
            </valueSetDefinition>
            <valueSettings>
                <controllingFieldValue>Escalation</controllingFieldValue>
                <valueName>Level 1</valueName>
            </valueSettings>
            <valueSettings>
                <controllingFieldValue>Escalation</controllingFieldValue>
                <valueName>Level 2</valueName>
