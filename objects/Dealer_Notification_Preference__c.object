    <validationRules>
        <fullName>Based_on_Escalation_Type</fullName>
        <active>false</active>
        <errorConditionFormula>$Setup.BypassAutomations__c.BypassValidationRule__c = False &amp;&amp; (TEXT(Type__c)=&apos;New Lead SMS&apos; || TEXT(Type__c)=&apos;All&apos; ) &amp;&amp;    ISBLANK(Phone__c)</errorConditionFormula>
        <errorMessage>Please add Phone Number</errorMessage>
    </validationRules>
    <validationRules>
        <fullName>only_numbers</fullName>
        <active>true</active>
        <errorConditionFormula>or(
      NOT(ISNUMBER( Phone__c  )),

      AND(
         LEN(Phone__c  ) != 10
         
     )
   )</errorConditionFormula>
        <errorMessage>Only 10 Digit number should be entered.</errorMessage>
    </validationRules>
